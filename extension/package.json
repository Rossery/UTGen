{
  "name": "cann-testcase-generator",
  "displayName": "CANN 测试用例生成器",
  "description": "基于 few-shot 与源码分析生成算子测试参数的 VS Code 扩展，封装运行 stage_1.py 并提供可视化界面。",
  "publisher": "local",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:cannTestcaseGenerator.open"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cannTestcaseGenerator.open",
        "title": "CANN 测试用例生成器: 打开面板"
      }
    ],
    "configuration": {
      "title": "CANN 测试用例生成器",
      "properties": {
        "cannTestcaseGenerator.pythonPath": {
          "type": "string",
          "default": "python3",
          "description": "Python 可执行文件路径（如 python 或 python3）"
        },
        "cannTestcaseGenerator.defaultScriptPath": {
          "type": "string",
          "default": "",
          "description": "入口脚本路径（留空则自动使用扩展目录的 entrypoint.sh）"
        },
        "cannTestcaseGenerator.useVirtualEnv": {
          "type": "boolean",
          "default": true,
          "description": "是否使用虚拟环境"
        },
        "cannTestcaseGenerator.venvPath": {
          "type": "string",
          "default": ".venv",
          "description": "虚拟环境目录路径（相对于工作区根目录）"
        }
      }
    }
  },
  "scripts": {
    "build": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "vscode:prepublish": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^18.19.0",
    "@types/vscode": "^1.88.0",
    "typescript": "^5.4.0"
  }
}

